! molokai inspired terminal colors
*foreground:#a0a0a0
*background:#1b1d1e
!black
*color0: #1b1d1e
*color8: #505354
!red
*color1: #f92672
*color9: #ff5995
!green
*color2: #82b414
*color10: #b6e354
!yellow
*color3: #fd971f
*color11: #feed6c
!blue
*color4: #56c2d6
*color12: #8cedff
!magenta
*color5: #8c54fe
*color13: #9e6ffe
!cyan
*color6: #465457
*color14: #899ca1
!white
*color7: #ccccc6
*celor15: #f8f8f2

! URXVT
URxvt*urgentOnBell: true
URxvt*internalBorder: 6
URxvt*cursorBlink: true
URxvt*scrollBar: false
URxvt*letterSpace: -4
URxvt.perl-ext-common: default,matcher,clipboard
URxvt.matcher.button: 1
URxvt.colorUL: #4682B4
URxvt.iso14755: False
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.clipboard.copycmd: xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
URxvt.iso14755_52: false
URxvt.keysym.C-Up: perl:font-size:increase
URxvt.keysym.C-Down: perl:font-size:decrease
URxvt.keysym.C-S-Up: perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal
URxvt*font:           xft:Source Code Pro:regular:size=9
URxvt*imFont:         xft:Source Code Pro:regular:size=9
URxvt*boldFont:       xft:Source Code Pro:semibold:size=9
URxvt*italicFont:     xft:Source Code Pro:italic:size=9
URxvt*boldItalicFont: xft:Source Code Pro:semibold:italic:size=9
