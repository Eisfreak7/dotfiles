# for more informaion and default bindings see the example bindings file

def_key "K"
  move_sort_order_up

def_key "K"
  move_selected_items_up

def_key "J"
  move_sort_order_down

def_key "J"
  move_selected_items_down

def_key "ctrl-k"
  select_item
  scroll_up

def_key "ctrl-j"
  select_item
  scroll_down

def_key "L"
  show_lyrics

def_key "k"
  scroll_up

def_key "j"
  scroll_down

def_key "v"
  select_item

def_key "ctrl-v"
  select_range

def_key "ctrl-u"
  page_up

def_key "ctrl-d"
  page_down

def_key "g"
  move_home

def_key "G"
  move_end

def_key "P"
  jump_to_playlist_editor

def_key "B"
  jump_to_browser

def_key "x"
  delete_playlist_items

def_key "x"
  delete_browser_items

def_key "x"
  delete_stored_playlist

def_key "l"
  next_column

def_key "l"
  slave_screen

def_key "l"
  volume_up

def_key "h"
  previous_column

def_key "h"
  master_screen

def_key "h"
  volume_down

def_key ","
  execute_command

def_key "ctrl-l"
  next_screen

def_key "ctrl-h"
  previous_screen

def_key "n"
  next_found_item

def_key "N"
  previous_found_item

def_key "c"
  clear_main_playlist

def_key "D"
  clear_playlist
